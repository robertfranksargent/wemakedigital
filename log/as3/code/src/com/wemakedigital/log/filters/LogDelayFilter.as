package com.wemakedigital.log.filters{	import flash.utils.getTimer;			public class LogDelayFilter implements ILogFilter	{		protected var className : String ;		protected var methodName : String ;		protected var level : int ;		protected var delay : int ;		protected var resetTime : int ;				public function get canOutputMessage ( ) : Boolean		{			return getTimer( ) - this.resetTime >= this.delay ;		}		public function LogDelayFilter ( delay : int ) : void		{			this.delay = delay ;		}				public function setClassName ( className : String ) : void		{			this.className = className ;		}		public function getClassName ( ) : String		{			return this.className ;		}				public function setMethodName ( methodName : String ) : void		{			this.methodName = methodName ;		}		public function getMethodName ( ) : String		{			return this.methodName ;		}				public function setLevel ( level : int ) : void		{			this.level = level ;		}				public function getLevel ( ) : int		{			return this.level ;		}				public function reset ( ) : void		{			this.resetTime = getTimer( ) ;		}	}}