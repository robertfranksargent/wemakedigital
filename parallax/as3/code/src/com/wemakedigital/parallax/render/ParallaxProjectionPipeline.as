package com.wemakedigital.parallax.render {	import com.wemakedigital.parallax.objects.ParallaxObject;				public class ParallaxProjectionPipeline 	{		public function project ( data : ParallaxRenderData ) : void		{			var renderList : Array = new Array( ) ;						for each ( var object : ParallaxObject in data.renderObjects )			{				if ( object.visible )				{					object.project( data ) ;					renderList.push( object ) ;				}			}						data.renderObjects = renderList ;		}	}}