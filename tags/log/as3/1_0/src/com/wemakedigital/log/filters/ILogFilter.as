package com.wemakedigital.log.filters{	/**	 * The interface implemented by log filters.	 */	public interface ILogFilter 	{		/**		 * Sets the class name.		 * 		 * @param	className	The name of the class to which the filter should be applied.		 */		function setClassName ( className : String ) : void ;				/**		 * Gets the class name.		 * 		 * @return	The name of the class to which the filter will be applied.		 */		function getClassName ( ) : String ;				/**		 * Sets the method name.		 * 		 * @param	methodName	The name of the method to which the filter should be applied.		 */		function setMethodName ( methodName : String ) : void ;				/**		 * Gets the method name.		 * 		 * @return	The name of the method to which the filter will be applied.		 */		function getMethodName ( ) : String ;				/**		 * Sets the logging level.		 * 		 * @param	level	The logging level to which the filter should be applied.		 */		function setLevel ( level : int ) : void ;				/**		 * Gets the logging level.		 * 		 * @return	The logging level to which the filter will be applied.		 */		function getLevel ( ) : int ; 				/**		 * Resets the filter.		 */		function reset ( ) : void ;				/**		 * @return	A value of <code>true</code> if a message can pass through the filter.		 */		function get canOutputMessage ( ) : Boolean ;	}}