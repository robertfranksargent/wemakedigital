package com.wemakedigital.state.example.gumballmachine {	import com.wemakedigital.log.Log;	import com.wemakedigital.log.LogLevel;	import com.wemakedigital.log.outputs.LogTrace;		import flash.display.Sprite;			/**	 * Gumball Machine demoing the capabilities of the Make Digital state 	 * machine framework.	 * 	 * @see com.wemakedigital.framework.state	 */	public class GumballMachine extends Sprite 	{		//----------------------------------------------------------------------		//		//  Constants		//		//----------------------------------------------------------------------				/**		 * Application multiton key.		 */		public static const APPLICATION_KEY : String = "com.wemakedigital.state.example.gumballmachine.GumballMachine";				//----------------------------------------------------------------------		//		//  Constructor		//		//----------------------------------------------------------------------				/**		 * Class contructor.		 */		public function GumballMachine ()		{			Log.addLogOutput( new LogTrace() ) ;			Log.setLevel( LogLevel.DEBUG ) ;			Log.stackTrace( this, "GumballMachine" ) ;			GumballMachineFacade.getInstance( GumballMachine.APPLICATION_KEY ).startup( this );		}	}}