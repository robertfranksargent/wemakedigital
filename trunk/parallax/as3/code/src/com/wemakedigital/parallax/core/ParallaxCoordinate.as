package com.wemakedigital.parallax.core {	public class ParallaxCoordinate 	{		public var x : Number ;		public var y : Number ;		public var z : Number ;				public function ParallaxCoordinate ( x : Number = 0 , y : Number = 0 , z : Number = 0 )		{			this.x = x ;			this.y = y ;			this.z = z ;		}				public function clone ( ) : ParallaxCoordinate		{			return new ParallaxCoordinate( this.x , this.y , this.z ) ;		}	}}