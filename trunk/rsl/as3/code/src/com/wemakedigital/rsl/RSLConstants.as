package com.wemakedigital.rsl {	/**	 * This class contains constants associated with the RSL package.	 */	public class RSLConstants 	{		//----------------------------------------------------------------------		//		//  Constants		//		//----------------------------------------------------------------------				/**		 * The RSLConstants.STARTUP constant defines the value of the <code>notificationName</code>		 * property for notifications which indicate the RSL application should be started up.		 */		public static const STARTUP : String = "RSLConstants.STARTUP" ;				/**		 * The RSLConstants.LOAD_MANIFEST constant defines the value of the		 * <code>notificationName</code> property for notifications which indicate that an RSL		 * manifest needs to be loaded in.		 * 		 * <p>The methods of the notification object return the following values:</p>		 * <table class=innertable>		 * <tr><th>Method</th><th>Value</th></tr>		 * <tr><td>getBody( )</td><td>The Manifest object that should be loaded in.</td></tr>		 * </table>		 * 		 * @eventType RSLConstants.LOAD_MANIFEST		 */		public static const LOAD_MANIFEST : String = "RSLConstants.LOAD_MANIFEST" ;				/**		 * The RSLConstants.LOAD_MANIFEST_COMPLETE constant defines the value of the		 * <code>notificationName</code> property for notifications which indicate that the loading		 * of an RSL manifest is complete.		 * 		 * <p>The methods of the notification object return the following values:</p>		 * <table class=innertable>		 * <tr><th>Method</th><th>Value</th></tr>		 * <tr><td>getBody( )</td><td>The Manifest object for which loading is complete.</td></tr>		 * </table>		 * 		 * @eventType RSLConstants.LOAD_MANIFEST_COMPLETE		 */		public static const LOAD_MANIFEST_COMPLETE : String = "RSLConstants.LOAD_MANIFEST_COMPLETE" ;				/**		 * The RSLConstants.LOAD_MANIFEST_ERROR constant defines the value of the		 * <code>notificationName</code> property for notifications which indicate that an error has		 * occurred while attempting to load an RSL manifest.		 * 		 * <p>The methods of the notification object return the following values:</p>		 * <table class=innertable>		 * <tr><th>Method</th><th>Value</th></tr>		 * <tr><td>getBody( )</td><td>The Manifest object which has caused the error.</td></tr>		 * </table>		 * 		 * @eventType RSLConstants.LOAD_MANIFEST_ERROR		 */		public static const LOAD_MANIFEST_ERROR : String = "RSLConstants.LOAD_MANIFEST_ERROR" ;				/**		 * The RSLConstants.LOAD_COMPONENT constant defines the value of the		 * <code>notificationName</code> property for notifications which indicate that an RSL		 * component needs to be loaded in.		 * 		 * <p>The methods of the notification object return the following values:</p>		 * <table class=innertable>		 * <tr><th>Method</th><th>Value</th></tr>		 * <tr><td>getBody( )</td><td>A Manifest object which describes the component.</td></tr>		 * </table>		 * 		 * @eventType RSLConstants.LOAD_COMPONENT		 */		public static const LOAD_COMPONENT : String = "RSLConstants.LOAD_COMPONENT" ;				/**		 * The RSLConstants.LOAD_COMPONENT_COMPLETE constant defines the value of the		 * <code>notificationName</code> property for notifications which indicate that the loading		 * of an RSL component is complete.		 * 		 * <p>The methods of the notification object return the following values:</p>		 * <table class=innertable>		 * <tr><th>Method</th><th>Value</th></tr>		 * <tr><td>getBody( )</td><td>The Manifest object for which loading is complete.</td></tr>		 * </table>		 * 		 * @eventType RSLConstants.LOAD_COMPONENT_COMPLETE		 */		public static const LOAD_COMPONENT_COMPLETE : String = "RSLConstants.LOAD_COMPONENT_COMPLETE" ;				/**		 * The RSLConstants.LOAD_COMPONENT_ERROR constant defines the value of the		 * <code>notificationName</code> property for notifications which indicate that an error has		 * occurred while attempting to load an RSL component.		 * 		 * <p>The methods of the notification object return the following values:</p>		 * <table class=innertable>		 * <tr><th>Method</th><th>Value</th></tr>		 * <tr><td>getBody( )</td><td>The Component which has caused the error.</td></tr>		 * </table>		 * 		 * @eventType RSLConstants.LOAD_COMPONENT_ERROR		 */		public static const LOAD_COMPONENT_ERROR : String = "RSLConstants.LOAD_COMPONENT_ERROR" ;	}}