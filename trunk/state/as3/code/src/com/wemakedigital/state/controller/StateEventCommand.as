package com.wemakedigital.state.controller {	import com.wemakedigital.state.model.StateProxy;		import com.wemakedigital.log.Log;		import org.puremvc.as3.multicore.interfaces.INotification;		/**	 * Calls state events in the state proxy.	 */	public class StateEventCommand extends SimpleStateCommand 	{				//----------------------------------------------------------------------		//		//  Methods		//		//----------------------------------------------------------------------				/**		 * Execute the command.		 */		override public function execute (notification : INotification) : void		{			Log.stackTrace( this, "execute", notification.getName(), notification.getType(), notification.getBody() ) ;						switch ( notification.getType() )			{				case StateProxy.CALL_CONFIRM :					this.stateProxy.callEvent( notification.getName( ), true ) ;					break;				case StateProxy.CONFIRM_RESPONSE :					this.stateProxy.confirmResponse( notification.getName(), notification.getBody() as Boolean ) ;					break;				default :							this.stateProxy.callEvent( notification.getName( ), false ) ;					break;			}		}	}}