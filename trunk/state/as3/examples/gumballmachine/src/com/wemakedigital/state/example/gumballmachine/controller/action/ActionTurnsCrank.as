package com.wemakedigital.state.example.gumballmachine.controller.action{	import com.wemakedigital.log.Log;	import com.wemakedigital.state.controller.SimpleStateCommand;	import com.wemakedigital.state.example.gumballmachine.model.GumballCounterProxy;		import org.puremvc.as3.multicore.interfaces.INotification;		/**	 * State machine action.	 */	public class ActionTurnsCrank extends SimpleStateCommand 	{			/** 		 * Execute the command.		 */		override public function execute (notification : INotification) : void		{			Log.stackTrace( this, "execute", notification.getName(), notification.getType(), notification.getBody() ) ;			var gumballCounterProxy : GumballCounterProxy = this.facade.retrieveProxy( GumballCounterProxy.NAME ) as GumballCounterProxy ;			gumballCounterProxy.gumballCount -- ;						stateProxy.actionResponse ( notification.getName() ) ;		}	}}